{
  "id": "symetrix-phonebook-processor",
  "manufacturer": "Symetrix",
  "model": "Phonebook Processor",
  "category": "dsp",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
        "getAll": "PURSS\r",
        "cmd2": "%u Send Info 2 %u %u\r",
        "cmd3": "SSYSS %u.1001.%u.%u.0=%s\r",
        "cmd4": "SSYSS %u.1000.%u.%u.0=%s\r"
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0
  },
  "dsp": {
    "maxChannels": 8
  },
  "feedbackRules": [
    {
      "pattern": "Send\\ Info\\\\r",
      "signal": "sTempDevice"
    },
    {
      "pattern": "SSYS",
      "signal": "sTempDevice"
    }
  ],
  "sourceModule": "Symetrix Phonebook Processor v3.0.usp"
}