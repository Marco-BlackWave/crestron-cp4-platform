{
  "id": "dsc-it100",
  "manufacturer": "DSC",
  "model": "IT-100 Serial Interface",
  "category": "security",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
        "statusRequest": "001\r\n",
        "labelsRequest": "002\r\n",
        "setDateTime": "010{hhmmMMDDYY}\r\n",
        "partitionArmAway1": "0301\r\n",
        "partitionArmAway2": "0302\r\n",
        "partitionArmStay1": "0311\r\n",
        "partitionArmStay2": "0312\r\n",
        "partitionArmNoEntryDelay1": "0321\r\n",
        "partitionArmNoEntryDelay2": "0322\r\n",
        "partitionArmWithCode1": "0331{code}\r\n",
        "partitionArmWithCode2": "0332{code}\r\n",
        "partitionDisarm1": "0401{code}\r\n",
        "partitionDisarm2": "0402{code}\r\n",
        "panicFire": "0601\r\n",
        "panicAmbulance": "0602\r\n",
        "panicPolice": "0603\r\n",
        "outputActivate1": "020111\r\n",
        "outputActivate2": "020121\r\n",
        "outputActivate3": "020131\r\n",
        "outputActivate4": "020141\r\n",
        "outputDeactivate1": "020110\r\n",
        "outputDeactivate2": "020120\r\n",
        "outputDeactivate3": "020130\r\n",
        "outputDeactivate4": "020140\r\n",
        "keypressKey0": "0701*\r\n",
        "keypressKey1": "07011\r\n",
        "keypressKey2": "07012\r\n",
        "keypressKey3": "07013\r\n",
        "keypressKey4": "07014\r\n",
        "keypressKey5": "07015\r\n",
        "keypressKey6": "07016\r\n",
        "keypressKey7": "07017\r\n",
        "keypressKey8": "07018\r\n",
        "keypressKey9": "07019\r\n",
        "keypressStar": "0701*\r\n",
        "keypressHash": "0701#\r\n",
        "codeRequired": "0501\r\n",
        "temperatureBroadcast": "0951\r\n",
        "triggerSmokeDetector": "0951\r\n"
      },
      "feedback": {
        "partitionState": { "poll": "001\r\n" }
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0,
    "maxZones": 64,
    "maxPartitions": 8
  },
  "feedbackRules": [
    { "pattern": "^500(\\d{3})$", "signal": "commandAck" },
    { "pattern": "^501$", "signal": "commandError" },
    { "pattern": "^502(\\d+)$", "signal": "systemError" },
    { "pattern": "^550(\\d{2})$", "signal": "timeDate" },
    { "pattern": "^601(\\d{3})(\\d)$", "signal": "zoneAlarm" },
    { "pattern": "^602(\\d{3})(\\d)$", "signal": "zoneAlarmRestore" },
    { "pattern": "^609(\\d{3})$", "signal": "zoneOpen" },
    { "pattern": "^610(\\d{3})$", "signal": "zoneRestored" },
    { "pattern": "^621(\\d{3})$", "signal": "zoneFire" },
    { "pattern": "^622(\\d{3})$", "signal": "zoneFireRestore" },
    { "pattern": "^650(\\d)$", "signal": "partitionReady" },
    { "pattern": "^651(\\d)$", "signal": "partitionNotReady" },
    { "pattern": "^652(\\d)$", "signal": "partitionArmed" },
    { "pattern": "^654(\\d)$", "signal": "partitionInAlarm" },
    { "pattern": "^655(\\d)$", "signal": "partitionDisarmed" },
    { "pattern": "^656(\\d)$", "signal": "exitDelay" },
    { "pattern": "^657(\\d)$", "signal": "entryDelay" },
    { "pattern": "^670(\\d)$", "signal": "invalidAccessCode" },
    { "pattern": "^672(\\d)$", "signal": "partitionArmedFailed" },
    { "pattern": "^700(\\d{3})(\\d)$", "signal": "userClosing" },
    { "pattern": "^750(\\d{3})(\\d)$", "signal": "userOpening" },
    { "pattern": "^800$", "signal": "panelBatteryTrouble" },
    { "pattern": "^801$", "signal": "panelBatteryRestore" },
    { "pattern": "^802$", "signal": "panelAcTrouble" },
    { "pattern": "^803$", "signal": "panelAcRestore" },
    { "pattern": "^900$", "signal": "codeRequired" }
  ],
  "sourceModule": "DSC IT-100 v1.0.usp"
}
