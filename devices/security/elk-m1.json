{
  "id": "elk-m1",
  "manufacturer": "Elk Products",
  "model": "M1 Gold / M1EZ8",
  "category": "security",
  "protocols": {
    "serial": {
      "baudRate": 115200,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
        "armAway": "0Da1{area}{code}00{checksum}\r\n",
        "armStay": "0Da3{area}{code}00{checksum}\r\n",
        "armStayInstant": "0Da4{area}{code}00{checksum}\r\n",
        "armNight": "0Da5{area}{code}00{checksum}\r\n",
        "armNightInstant": "0Da6{area}{code}00{checksum}\r\n",
        "armVacation": "0Da7{area}{code}00{checksum}\r\n",
        "disarm": "0Da0{area}{code}00{checksum}\r\n",
        "armingStatusRequest": "06as00{checksum}\r\n",
        "zoneStatusRequest": "06zs00{checksum}\r\n",
        "zoneDefinitionRequest": "06zd00{checksum}\r\n",
        "zonePartitionRequest": "06zp00{checksum}\r\n",
        "zoneVoltageRequest": "06zv{zone}{checksum}\r\n",
        "outputStatusRequest": "06cs00{checksum}\r\n",
        "outputOn": "09cn{output}0000{checksum}\r\n",
        "outputOff": "09cf{output}0000{checksum}\r\n",
        "outputToggle": "09ct{output}0000{checksum}\r\n",
        "keypressKey0": "09kf{keypad}00{checksum}\r\n",
        "keypressKey1": "09kf{keypad}01{checksum}\r\n",
        "keypressKey2": "09kf{keypad}02{checksum}\r\n",
        "keypressKey3": "09kf{keypad}03{checksum}\r\n",
        "keypressKey4": "09kf{keypad}04{checksum}\r\n",
        "keypressKey5": "09kf{keypad}05{checksum}\r\n",
        "keypressKey6": "09kf{keypad}06{checksum}\r\n",
        "keypressKey7": "09kf{keypad}07{checksum}\r\n",
        "keypressKey8": "09kf{keypad}08{checksum}\r\n",
        "keypressKey9": "09kf{keypad}09{checksum}\r\n",
        "keypressStar": "09kf{keypad}10{checksum}\r\n",
        "keypressHash": "09kf{keypad}11{checksum}\r\n",
        "thermostatRequest": "06tr{thermostat}{checksum}\r\n",
        "thermostatSetCool": "0Bts{thermostat}0{temp}{checksum}\r\n",
        "thermostatSetHeat": "0Bts{thermostat}1{temp}{checksum}\r\n",
        "thermostatModeOff": "0Bts{thermostat}20{checksum}\r\n",
        "thermostatModeHeat": "0Bts{thermostat}21{checksum}\r\n",
        "thermostatModeCool": "0Bts{thermostat}22{checksum}\r\n",
        "thermostatModeAuto": "0Bts{thermostat}23{checksum}\r\n",
        "lightingOn": "09pn{device}{checksum}\r\n",
        "lightingOff": "09pf{device}{checksum}\r\n",
        "lightingToggle": "09pt{device}{checksum}\r\n",
        "lightingLevel": "09pc{device}{level}{checksum}\r\n",
        "taskActivate": "09tn{task}00{checksum}\r\n",
        "speakWord": "0Bsp{device}{word}{checksum}\r\n",
        "speakPhrase": "0Bsp{device}{phrase}{checksum}\r\n",
        "textDescriptionRequest": "0Bsd{type}{address}{checksum}\r\n",
        "dateTimeRequest": "06rr00{checksum}\r\n",
        "versionRequest": "06vn00{checksum}\r\n",
        "counterValueRead": "09cv{counter}0{checksum}\r\n",
        "counterValueWrite": "0Bcv{counter}{value}{checksum}\r\n",
        "customValueRead": "09cr{custom}0{checksum}\r\n",
        "customValueWrite": "0Dcw{custom}{value}{checksum}\r\n"
      },
      "feedback": {
        "armingStatus": { "poll": "06as00{checksum}\r\n" },
        "zoneStatus": { "poll": "06zs00{checksum}\r\n" },
        "outputStatus": { "poll": "06cs00{checksum}\r\n" }
      }
    },
    "ip": {
      "port": 2101,
      "type": "tcp",
      "commands": {
        "armAway": "0Da1{area}{code}00{checksum}\r\n",
        "armStay": "0Da3{area}{code}00{checksum}\r\n",
        "armNight": "0Da5{area}{code}00{checksum}\r\n",
        "disarm": "0Da0{area}{code}00{checksum}\r\n",
        "armingStatusRequest": "06as00{checksum}\r\n",
        "zoneStatusRequest": "06zs00{checksum}\r\n",
        "outputOn": "09cn{output}0000{checksum}\r\n",
        "outputOff": "09cf{output}0000{checksum}\r\n",
        "taskActivate": "09tn{task}00{checksum}\r\n"
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0,
    "maxZones": 208,
    "maxAreas": 8,
    "maxOutputs": 208,
    "maxThermostats": 16,
    "maxTasks": 32
  },
  "feedbackRules": [
    { "pattern": "^AS(.{8})(.{8})(.{8})$", "signal": "armingStatus" },
    { "pattern": "^ZC(\\d{3})(.)(.)$", "signal": "zoneChange" },
    { "pattern": "^ZS(.{208})$", "signal": "zoneStatusAll" },
    { "pattern": "^CC(\\d{3})(.)$", "signal": "outputChange" },
    { "pattern": "^TC(\\d{2})$", "signal": "taskChange" },
    { "pattern": "^IC(\\d)(\\d)(.)$", "signal": "invalidCode" },
    { "pattern": "^EE(\\d{3})(\\d)(\\d)(\\d{3})$", "signal": "entryExitTimer" },
    { "pattern": "^AR(\\d)$", "signal": "alarmReporting" },
    { "pattern": "^TR(\\d{2})(.{2})(.{2})(.{2})(.)(.)$", "signal": "thermostatData" },
    { "pattern": "^PC(\\d{3})(.{3})$", "signal": "plcChange" },
    { "pattern": "^RR(.{16})$", "signal": "dateTime" },
    { "pattern": "^VN(.+)$", "signal": "version" },
    { "pattern": "^SD(.{2})(.{3})(.{16})$", "signal": "textDescription" },
    { "pattern": "^CV(\\d{2})(.{5})$", "signal": "counterValue" },
    { "pattern": "^CR(\\d{2})(.{5})$", "signal": "customValue" },
    { "pattern": "^KA(.{16})$", "signal": "keypadAreaAssignment" }
  ],
  "sourceModule": "Elk M1 v2.0.usp"
}
