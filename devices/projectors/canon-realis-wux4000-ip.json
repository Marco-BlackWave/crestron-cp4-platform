        "sharpnessUp": "SHARP=%d%s",
        "sharpnessDown": "SHARP=%d%s",
        "saturationUp": "SAT=%d%s",
        "saturationDown": "SAT=%d%s",
        "hueUp": "HUE=%d%s",
        "hueDown": "HUE=%d%s",
        "volumeUp": "AVOL=%d%s",
        "volumeDown": "AVOL=%d%s"
      }
    }
  },
  "capabilities": {
    "discretePower": false,
