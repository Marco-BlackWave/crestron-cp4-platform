      "signal": "sResponseTemp"
    },
    {
      "pattern": "g:ASPECT=",
      "signal": "sResponseTemp"
    },
    {
      "pattern": "g:MUTE=",
      "signal": "sResponseTemp"
    },
    {
      "pattern": "g:LAMP=",
      "signal": "sResponseTemp"
    },
    {
      "pattern": "g:IMAGE=",
      "signal": "sResponseTemp"
    }
  ],
  "sourceModule": "Canon REALiS WUX4000 IP v1.0 Processor.usp"
