        "cursorUp": "0,1,*,PUT,%s(List_Control(Cursor=Up)),*\r\n",
        "cursorDown": "0,1,*,PUT,%s(List_Control(Cursor=Down)),*\r\n",
        "cursorRight": "0,1,*,PUT,%s(List_Control(Cursor=Right)),*\r\n",
        "cursorLeft": "0,1,*,PUT,%s(List_Control(Cursor=Left)),*\r\n",
        "enter": "0,1,*,PUT,%s(List_Control(Cursor=Sel)),*\r\n",
        "pageUp": "0,1,*,PUT,%s(List_Control(Page=Up)),*\r\n",
        "pageDown": "0,1,*,PUT,%s(List_Control(Page=Down)),*\r\n"
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
