{
  "id": "yamaha-rx-z7-list-control",
  "manufacturer": "Yamaha",
  "model": "RX-Z7 List Control",
  "category": "audio",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
