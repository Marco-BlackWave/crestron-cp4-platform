{
  "id": "symetrix-volume-processor",
  "manufacturer": "Symetrix",
  "model": "Volume Processor",
  "category": "audio",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
        "volUp": "CS %s %u\r",
        "volDown": "CS %s %u\r",
        "cmd3": "%u Send Info 0 %s\r",
        "cmd4": "%d dB"
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": true,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0
  },
  "feedbackRules": [
    {
      "pattern": "Send\\ Info\\\\r",
      "signal": "From_Command_Processor"
    }
  ],
  "sourceModule": "Symetrix Volume Processor v3.1.usp"
}