    },
    {
      "pattern": "\\\\xAA\\\\xAA",
      "signal": "LocalString"
    },
    {
      "pattern": "\\\\xAA\\\\xAA\\\\x00\\\\x00",
      "signal": "LocalString"
    }
  ],
  "sourceModule": "RAD_TE_Comm_Rev01.usp"
