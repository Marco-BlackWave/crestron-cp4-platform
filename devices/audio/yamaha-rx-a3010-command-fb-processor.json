{
  "id": "yamaha-rx-a3010",
  "manufacturer": "Yamaha",
  "model": "RX-A3010",
  "category": "audio",
  "protocols": {
    "ip": {
      "port": 50000,
      "type": "tcp",
      "commands": {
        "powerOn": "@MAIN:PWR=On\r\n",
        "powerOff": "@MAIN:PWR=Standby\r\n",
        "powerQuery": "@MAIN:PWR=?\r\n",
        "volumeUp": "@MAIN:VOL=Up\r\n",
        "volumeDown": "@MAIN:VOL=Down\r\n",
        "volumeQuery": "@MAIN:VOL=?\r\n",
        "muteOn": "@MAIN:MUTE=On\r\n",
        "muteOff": "@MAIN:MUTE=Off\r\n",
        "muteQuery": "@MAIN:MUTE=?\r\n",
        "inputHdmi1": "@MAIN:INP=HDMI1\r\n",
        "inputHdmi2": "@MAIN:INP=HDMI2\r\n",
        "inputHdmi3": "@MAIN:INP=HDMI3\r\n",
        "inputHdmi4": "@MAIN:INP=HDMI4\r\n",
        "inputHdmi5": "@MAIN:INP=HDMI5\r\n",
        "inputHdmi6": "@MAIN:INP=HDMI6\r\n",
        "inputHdmi7": "@MAIN:INP=HDMI7\r\n",
        "inputAv1": "@MAIN:INP=AV1\r\n",
        "inputAv2": "@MAIN:INP=AV2\r\n",
        "inputAv3": "@MAIN:INP=AV3\r\n",
        "inputAudio1": "@MAIN:INP=AUDIO1\r\n",
        "inputAudio2": "@MAIN:INP=AUDIO2\r\n",
        "inputTuner": "@MAIN:INP=TUNER\r\n",
        "inputUsb": "@MAIN:INP=USB\r\n",
        "inputNet": "@MAIN:INP=NET RADIO\r\n",
        "inputQuery": "@MAIN:INP=?\r\n",
        "soundProgramStraight": "@MAIN:SOUNDPRG=Straight\r\n",
        "soundProgramSurDecode": "@MAIN:SOUNDPRG=Sur.Decode\r\n",
        "soundProgram7chStereo": "@MAIN:SOUNDPRG=7ch Stereo\r\n",
        "soundProgramHallMunich": "@MAIN:SOUNDPRG=Hall in Munich\r\n",
        "soundProgramQuery": "@MAIN:SOUNDPRG=?\r\n",
        "sleepOff": "@MAIN:SLEEP=Off\r\n",
        "sleep30": "@MAIN:SLEEP=30 min\r\n",
        "sleep60": "@MAIN:SLEEP=60 min\r\n",
        "sleep90": "@MAIN:SLEEP=90 min\r\n",
        "sleep120": "@MAIN:SLEEP=120 min\r\n",
        "sleepQuery": "@MAIN:SLEEP=?\r\n",
        "pureDirectOn": "@MAIN:PUREDIRMODE=On\r\n",
        "pureDirectOff": "@MAIN:PUREDIRMODE=Off\r\n",
        "enhancerOn": "@MAIN:ENHANCER=On\r\n",
        "enhancerOff": "@MAIN:ENHANCER=Off\r\n",
        "zone2PowerOn": "@ZONE2:PWR=On\r\n",
        "zone2PowerOff": "@ZONE2:PWR=Standby\r\n",
        "zone2VolumeUp": "@ZONE2:VOL=Up\r\n",
        "zone2VolumeDown": "@ZONE2:VOL=Down\r\n",
        "zone2MuteOn": "@ZONE2:MUTE=On\r\n",
        "zone2MuteOff": "@ZONE2:MUTE=Off\r\n",
        "zone2Query": "@ZONE2:PWR=?\r\n",
        "zone3PowerOn": "@ZONE3:PWR=On\r\n",
        "zone3PowerOff": "@ZONE3:PWR=Standby\r\n",
        "zone3VolumeUp": "@ZONE3:VOL=Up\r\n",
        "zone3VolumeDown": "@ZONE3:VOL=Down\r\n",
        "zone3Query": "@ZONE3:PWR=?\r\n",
        "zone4PowerOn": "@ZONE4:PWR=On\r\n",
        "zone4PowerOff": "@ZONE4:PWR=Standby\r\n",
        "zone4Query": "@ZONE4:PWR=?\r\n",
        "sceneRecall1": "@MAIN:SCENE=Scene 1\r\n",
        "sceneRecall2": "@MAIN:SCENE=Scene 2\r\n",
        "sceneRecall3": "@MAIN:SCENE=Scene 3\r\n",
        "sceneRecall4": "@MAIN:SCENE=Scene 4\r\n",
        "tunerFmFreq": "@TUNER:FMFREQ={frequency}\r\n",
        "tunerAmFreq": "@TUNER:AMFREQ={frequency}\r\n",
        "tunerPresetUp": "@TUNER:PRESET=Up\r\n",
        "tunerPresetDown": "@TUNER:PRESET=Down\r\n",
        "netRadioPresetUp": "@NETRADIO:LISTSEL=Up\r\n",
        "netRadioPresetDown": "@NETRADIO:LISTSEL=Down\r\n",
        "netRadioSelect": "@NETRADIO:LISTSEL=Select\r\n",
        "netRadioReturn": "@NETRADIO:LISTSEL=Return\r\n",
        "playbackPlay": "@MAIN:PLAYBACK=Play\r\n",
        "playbackPause": "@MAIN:PLAYBACK=Pause\r\n",
        "playbackStop": "@MAIN:PLAYBACK=Stop\r\n",
        "playbackNext": "@MAIN:PLAYBACK=Skip Fwd\r\n",
        "playbackPrev": "@MAIN:PLAYBACK=Skip Rev\r\n"
      }
    }
  },
  "capabilities": {
    "discretePower": true,
    "volumeControl": true,
    "inputSelect": true,
    "feedback": true,
    "warmupMs": 2000,
    "cooldownMs": 0
  },
  "feedbackRules": [
    { "pattern": "^@MAIN:PWR=On$", "signal": "power", "value": true },
    { "pattern": "^@MAIN:PWR=Standby$", "signal": "power", "value": false },
    { "pattern": "^@MAIN:VOL=(-?[\\d.]+)$", "signal": "volume" },
    { "pattern": "^@MAIN:MUTE=On$", "signal": "mute", "value": true },
    { "pattern": "^@MAIN:MUTE=Off$", "signal": "mute", "value": false },
    { "pattern": "^@MAIN:INP=(.+)$", "signal": "inputSource" },
    { "pattern": "^@MAIN:SOUNDPRG=(.+)$", "signal": "soundProgram" },
    { "pattern": "^@MAIN:SLEEP=(.+)$", "signal": "sleepTimer" },
    { "pattern": "^@ZONE2:PWR=On$", "signal": "zone2Power", "value": true },
    { "pattern": "^@ZONE2:PWR=Standby$", "signal": "zone2Power", "value": false },
    { "pattern": "^@ZONE2:VOL=(-?[\\d.]+)$", "signal": "zone2Volume" },
    { "pattern": "^@ZONE3:PWR=On$", "signal": "zone3Power", "value": true },
    { "pattern": "^@ZONE3:PWR=Standby$", "signal": "zone3Power", "value": false },
    { "pattern": "^@ZONE4:PWR=On$", "signal": "zone4Power", "value": true },
    { "pattern": "^@ZONE4:PWR=Standby$", "signal": "zone4Power", "value": false },
    { "pattern": "^@MAIN:SCENE=(.+)$", "signal": "currentScene" }
  ],
  "sourceModule": "Yamaha RX-A3010 Command & FB Processor v1.0.usp"
}
