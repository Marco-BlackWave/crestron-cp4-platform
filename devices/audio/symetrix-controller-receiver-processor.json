  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0
  },
  "feedbackRules": [
    {
      "pattern": "Send\\ Info\\\\r",
      "signal": "sTempDevice"
    },
    {
      "pattern": "\\\\x23",
      "signal": "sTempDevice"
    },
    {
      "pattern": "=",
      "signal": "sTempDevice"
    }
  ],
  "sourceModule": "Symetrix Controller Receiver Processor v3.0.usp"
}