      "signal": "avr_rx$"
    },
    {
      "pattern": "Z3PSBAS",
      "signal": "avr_rx$"
    },
    {
      "pattern": "Z3PSTRE",
      "signal": "avr_rx$"
    },
    {
      "pattern": "PS",
      "signal": "avr_rx$"
    },
    {
      "pattern": "Z2PS",
      "signal": "avr_rx$"
    },
    {
      "pattern": "Z3PS",
      "signal": "avr_rx$"
    }
  ],
  "sourceModule": "Denon_Equalizer_V1.1_AZ.usp"
}