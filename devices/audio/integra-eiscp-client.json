{
  "id": "integra-eiscp-client",
  "manufacturer": "Integra/Onkyo",
  "model": "eISCP Network Client",
  "category": "audio",
  "protocols": {
    "ip": {
      "port": 60128,
      "type": "tcp",
      "commands": {
        "powerOn": "!1PWR01\r",
        "powerOff": "!1PWR00\r",
        "powerQuery": "!1PWRQSTN\r",
        "volumeUp": "!1MVLUP\r",
        "volumeDown": "!1MVLDOWN\r",
        "volumeQuery": "!1MVLQSTN\r",
        "muteOn": "!1AMT01\r",
        "muteOff": "!1AMT00\r",
        "muteToggle": "!1AMTTG\r",
        "muteQuery": "!1AMTQSTN\r",
        "inputCblSat": "!1SLI01\r",
        "inputGame": "!1SLI02\r",
        "inputAux": "!1SLI03\r",
        "inputPc": "!1SLI05\r",
        "inputBluray": "!1SLI10\r",
        "inputStrm": "!1SLI11\r",
        "inputTv": "!1SLI12\r",
        "inputPhono": "!1SLI22\r",
        "inputCd": "!1SLI23\r",
        "inputTuner": "!1SLI26\r",
        "inputUsb": "!1SLI29\r",
        "inputNet": "!1SLI2B\r",
        "inputBluetooth": "!1SLI2E\r",
        "inputQuery": "!1SLIQSTN\r",
        "listeningModeStereo": "!1LMD00\r",
        "listeningModeDirect": "!1LMD01\r",
        "listeningModeSurround": "!1LMD02\r",
        "listeningModeTheater": "!1LMD0E\r",
        "listeningModeAllChStereo": "!1LMD0C\r",
        "listeningModeQuery": "!1LMDQSTN\r",
        "zone2PowerOn": "!1ZPW01\r",
        "zone2PowerOff": "!1ZPW00\r",
        "zone2VolumeUp": "!1ZVLUP\r",
        "zone2VolumeDown": "!1ZVLDOWN\r",
        "zone2MuteOn": "!1ZMT01\r",
        "zone2MuteOff": "!1ZMT00\r",
        "zone2Query": "!1ZPWQSTN\r",
        "playResume": "!1NTCPLAY\r",
        "playPause": "!1NTCPAUSE\r",
        "playStop": "!1NTCSTOP\r",
        "playNext": "!1NTCTRUP\r",
        "playPrev": "!1NTCTRDN\r",
        "infoQuery": "!1NFIQSTN\r"
      }
    }
  },
  "capabilities": {
    "discretePower": true,
    "volumeControl": true,
    "inputSelect": true,
    "feedback": true,
    "warmupMs": 2000,
    "cooldownMs": 0
  },
  "feedbackRules": [
    { "pattern": "^!1PWR(\\d{2})$", "signal": "power" },
    { "pattern": "^!1MVL([0-9A-F]{2})$", "signal": "volume" },
    { "pattern": "^!1AMT(\\d{2})$", "signal": "mute" },
    { "pattern": "^!1SLI([0-9A-F]{2})$", "signal": "inputSource" },
    { "pattern": "^!1LMD([0-9A-F]{2})$", "signal": "listeningMode" },
    { "pattern": "^!1ZPW(\\d{2})$", "signal": "zone2Power" },
    { "pattern": "^!1ZVL([0-9A-F]{2})$", "signal": "zone2Volume" },
    { "pattern": "^!1ZMT(\\d{2})$", "signal": "zone2Mute" },
    { "pattern": "^!1NTR(.+)$", "signal": "networkTransport" },
    { "pattern": "^!1NFI(.+)$", "signal": "networkInfo" },
    { "pattern": "^!1NTI(.+)$", "signal": "trackInfo" },
    { "pattern": "^!1NAL(.+)$", "signal": "albumInfo" },
    { "pattern": "^!1NAT(.+)$", "signal": "artistInfo" }
  ],
  "sourceModule": "Integra eISCP Client v0.1.usp"
}
