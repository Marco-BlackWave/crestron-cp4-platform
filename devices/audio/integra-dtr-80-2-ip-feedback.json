    {
      "pattern": "\\\\x1A",
      "signal": "From_Device$"
    },
    {
      "pattern": "\\\\x0D",
      "signal": "From_Device$"
    },
    {
      "pattern": "\\\\x0D",
      "signal": "From_Device$"
    },
    {
      "pattern": "\\\\x1A",
      "signal": "sTemp"
    },
    {
      "pattern": "\\\\x1A",
      "signal": "sTemp"
    }
  ],
  "sourceModule": "Integra DTR-80.2 IP Feedback v1.0.usp"
