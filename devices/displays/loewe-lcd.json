{
  "id": "loewe-lcd",
  "manufacturer": "Loewe",
  "model": "LCD",
  "category": "audio",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": true,
    "inputSelect": true,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0
  },
  "feedbackRules": [
    {
      "pattern": "\u003E",
      "signal": "Rx$"
    },
    {
      "pattern": "\\\\x0D\\\\x0A",
      "signal": "tempString"
    },
    {
      "pattern": "\\\\x22",
      "signal": "tempString"
    }
  ],
  "sourceModule": "Loewe LCD.usp"
}