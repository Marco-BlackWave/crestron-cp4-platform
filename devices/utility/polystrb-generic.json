{
  "id": "polystrb-generic",
  "manufacturer": "PolystrB",
  "model": "Generic",
  "category": "utility",
  "protocols": {
    "serial": {
      "baudRate": 9600,
      "dataBits": 8,
      "parity": "none",
      "stopBits": 1,
      "commands": {
        "sendaudioport": "streamaudioport set ",
        "sendvideoport": "streamvideoport set ",
        "sendmulticastip": "streammulticastip set "
      }
    }
  },
  "capabilities": {
    "discretePower": false,
    "volumeControl": false,
    "inputSelect": false,
    "feedback": true,
    "warmupMs": 0,
    "cooldownMs": 0
  },
  "feedbackRules": [
    {
      "pattern": "\\\\x0D\\\\x0A",
      "signal": "FromDevice$"
    },
    {
      "pattern": "\\\\x0A\\\\x0D",
      "signal": "FromDevice$"
    },
    {
      "pattern": "\\\\x0D\\\\x0A",
      "signal": "FromDevice$"
    },
    {
      "pattern": "\\\\x0A\\\\x0D",
      "signal": "FromDevice$"
    },
    {
      "pattern": "streamaudioport\\ ",
      "signal": "Tempstring$"
    },
    {
      "pattern": "\\ get",
      "signal": "Tempstring$"
    },
    {
      "pattern": "streamvideoport\\ ",
      "signal": "Tempstring$"
    },
    {
      "pattern": "\\ get",
      "signal": "Tempstring$"
    },
    {
      "pattern": "multicastip\\ ",
      "signal": "Tempstring$"
    },
    {
      "pattern": "\\ get",
      "signal": "Tempstring$"
    }
  ],
  "sourceModule": "PolystrB.usp"
}