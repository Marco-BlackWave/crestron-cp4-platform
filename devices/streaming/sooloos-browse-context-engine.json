    },
    {
      "pattern": "UNMUTED",
      "signal": "tir"
    },
    {
      "pattern": "\u003CZ\u003E",
      "signal": "command"
    }
  ],
  "sourceModule": "Sooloos Browse Context Engine v2.5.usp"
}